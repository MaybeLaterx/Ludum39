// LAYOUT MUST BE COMPLETELY ENCLOSED BY X'S

layout = global.layout;

trace("GENERATING LEVEL
Selected level: 

" + layout); 


width = getLayoutWidth(layout);
height = getLayoutHeight(layout); 

//room_height = height*size; 
//room_width = width*size;

x = room_width/2; 
y = room_height/2; 

grid = ds_grid_create(width,height);

for (i = 0; i &lt; height; i++) { 
    for (j = 0; j &lt; width; j++) { 
        pos = 1 + i*(width+1) + j;
        //trace(pos);
        thisChar = string_char_at(layout,pos);//1 + height*width + width);
        if (thisChar != "#") { 
            //trace(thisChar);
            //if (thisChar == "O") glyph = "O"; 
            //else glyph = "X";
            ds_grid_set(grid,j,i,thisChar);
        }
    }
}

xOffset = x - width*size/2;
yOffset = y - height*size/2;

for (i = 0; i &lt; height; i++) { 
    //trace(i);
    //trace("Height:" + string(height)); 
    for (j = 0; j &lt; width; j++) { 
        tile = ds_grid_get(grid,j,i);
        xx = xOffset + j*size; 
        yy = yOffset + i*size;
        
        switch (tile) { 
            case "H" : instance_create(xx,yy,oHuman); break;
            case "B" : instance_create(xx,yy,oBattery); break;
            case "0" : instance_create(xx,yy,oFloor); break;
            case "1" : instance_create(xx,yy,oShadow1); break;
            case "2" : instance_create(xx,yy,oShadow2); break;
            case "3" : instance_create(xx,yy,oShadow3); break;
            case "X" : default : instance_create(xx,yy,oWall); break;
        } 
    }
}

ds_grid_destroy(grid); 
instance_destroy(); 
</string>
