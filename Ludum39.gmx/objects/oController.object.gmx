        updateScopes(oShadow2); 
        updateBatteryProximity(7); 
        // updateWallProximity (so expensive!) 

        needMove = attemptMove(2); 
        if (!needMove) { 
            needShoot = attemptShoot(true); 
            if (!needShoot) { 
                wantShoot = attemptShoot(false);
                if (!wantShoot) { 
                    shouldMove = attemptMove(1); 
                    if (!shouldMove) { 
                        wantMove = attemptMove(0); 
                        if (!wantMove) {
                            oController.powersLeft = 0; 
                            oController.movesLeft = 0;     
                        }
                    }
                }
            }
        }
        
        timer = maxTimer; 
    }
}
